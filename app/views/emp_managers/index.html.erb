<p id="notice"><%= notice %></p>

<h1>Emp Managers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emp_managers.each do |emp_manager| %>
      <tr>
        <td><%= emp_manager.first_name %></td>
        <td><%= emp_manager.last_name %></td>
        <td><%= link_to 'Show', emp_manager %></td>
        <td><%= link_to 'Edit', edit_emp_manager_path(emp_manager) %></td>
        <td><%= link_to 'Destroy', emp_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Emp Manager', new_emp_manager_path %>
